{
  "name": "code4u",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "deploy": "npm run build && npm run sync-to-s3 && npm run invalidate-cloudfront",
    "build": "vite build",
    "sync-to-s3": "aws s3 sync dist/ s3://code4u.innoaya.org/ --delete",
    "invalidate-cloudfront": "aws cloudfront create-invalidation --distribution-id E2GFBUCWIF2RUF --paths '/*'",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "firebase": "^11.6.1",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "globals": "^16.0.0",
    "postcss": "^8.4.23",
    "prettier": "3.5.3",
    "tailwindcss": "^3.3.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2"
  }
}
